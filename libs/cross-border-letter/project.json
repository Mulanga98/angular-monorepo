{
  "name": "cross-border-letter",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/cross-border-letter/src",
  "prefix": "angular-monorepo",
  "tags": [],
  "projectType": "library",
  "targets": {
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/cross-border-letter/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "libs/cross-border-letter/**/*.ts",
          "libs/cross-border-letter/**/*.html"
        ]
      }
    },
    "component-test": {
      "executor": "@nx/cypress:cypress",
      "options": {
        "cypressConfig": "libs/cross-border-letter/cypress.config.ts",
        "testingType": "component",
        "skipServe": true,
        "devServerTarget": "angular-monorepo:build"
      }
    }
  }
}
